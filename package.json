    "sanitize-filename": "1.6.3",
    "subtitle": "4.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/node": "14.11.5",
    "rimraf": "3.0.2",
    "typescript": "4.0.3"
  },
  "scripts": {
    "prepare": "rimraf dist && tsc",
    "start": "node ."
