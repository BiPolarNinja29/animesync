{
  "author": "Roel van Uden",
  "description": "AnimeSync is capable of downloading anime episodes from popular streaming services.",
  "license": "MIT",
  "main": "dist",
  "name": "animesync",
  "repository": "git://github.com/Deathspike/animesync",
  "types": "dist",
  "version": "0.4.1",
